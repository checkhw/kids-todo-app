{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "jsx": "react-jsx",
    "moduleResolution": "Node16",
    "allowImportingTsExtensions": true,
    "noEmit": true, // ✅ これを追加することでエラー回避！
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}